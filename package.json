{
    "name": "file-renamer",
    "description": "File Renamer: Rename all files inside a folder based on prefix or show name.",
    "author": "Abhijith P",
    "version": "0.1.15",
    "main": "main.js",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "electron": "ng build --base-href ./ && electron .",
        "stg": " electron .",
        "pack": "electron-builder --dir",
        "dist": "electron-builder"
    },
    "build": {
        "directories": {
            "output": "release/"
        }
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged && ng lint",
            "pre-push": "ng build --aot true"
        }
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^10.0.1",
        "@angular/cdk": "^10.0.0",
        "@angular/common": "~10.0.1",
        "@angular/compiler": "~10.0.1",
        "@angular/core": "~10.0.1",
        "@angular/forms": "~10.0.1",
        "@angular/material": "^10.0.0",
        "@angular/platform-browser": "~10.0.1",
        "@angular/platform-browser-dynamic": "~10.0.1",
        "@angular/router": "~10.0.1",
        "bootstrap": "^4.5.0",
        "core-js": "^3.6.5",
        "hammerjs": "^2.0.8",
        "ngx-electron": "^2.2.0",
        "ngx-fs": "0.0.1",
        "rxjs": "~6.5.5",
        "rxjs-compat": "^6.5.5",
        "tslint-eslint-rules": "^5.4.0",
        "zone.js": "~0.10.3"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^0.1000.0",
        "@angular/cli": "~10.0.0",
        "@angular/compiler-cli": "~10.0.1",
        "@angular/language-service": "~10.0.1",
        "@types/jasmine": "~3.5.11",
        "@types/jasminewd2": "~2.0.8",
        "@types/node": "~14.0.14",
        "codelyzer": "~5.2.2",
        "electron": "^9.0.5",
        "electron-builder": "^22.7.0",
        "husky": "^4.2.5",
        "jasmine-core": "~3.5.0",
        "jasmine-spec-reporter": "~5.0.2",
        "karma": "~5.1.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~3.0.3",
        "karma-jasmine": "~3.3.1",
        "karma-jasmine-html-reporter": "^1.5.4",
        "prettier": "^2.0.5",
        "pretty-quick": "^2.0.1",
        "protractor": "~7.0.0",
        "ts-node": "~8.10.2",
        "tslint": "~6.1.2",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "~3.9.5"
    }
}
